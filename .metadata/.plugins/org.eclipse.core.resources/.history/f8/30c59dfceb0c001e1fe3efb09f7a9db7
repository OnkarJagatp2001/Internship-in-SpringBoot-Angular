package com.demo;

public class Main {
	public static void main(String[] args)
	{
		int a = 1000;
		int b = 0;
		
		try
		{
			int ans = a / b;
			System.out.println(ans);
			throw ArrayIndexOutOfBoundsException;
		}catch(Exception e)
		{
			System.out.println("Exception is: "+ e);
		}finally {
			System.out.println("This is finally block");
		}
		
		
	}

}
