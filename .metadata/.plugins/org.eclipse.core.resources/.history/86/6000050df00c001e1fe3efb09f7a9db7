package com.demo;

public class ArrayException {
	public static void main()
	{
		int arr[] = new int[4];
		
		for(int i=0;i<4;i++)
		{
			arr[i]=(int)(Math.random()*10);
		}
		
		try {
			for(int i=0;i<4;i++) {
				System.out.println(arr[i]);
			}
		}catch(Exception e)
		{
			System.out.println("Exception is: "+ e);
		}
		finally {
			System.out.println()
		}
	}

}
