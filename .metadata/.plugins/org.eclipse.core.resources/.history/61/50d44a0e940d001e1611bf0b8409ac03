package Demo;

public class TreadDemoClass extends Thread{

	Thread t;
	TreadDemoClass(String name)
	{
		t = new Thread();
		t.start();
		System.out.println(name + " Thread");
	}
	
	public void run()
	{
		for(int i=0;i<20;i++)
		{
			System.out.println(t.getName() + " " + i);
		}
	}
	
	public static void main(String args[])
	{
		
	}
}
